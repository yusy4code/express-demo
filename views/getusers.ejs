<!DOCTYPE html>
<html>

<head>
    <style>
        body {
            background-color: #ccc;
            font-family: Arial, Helvetica, sans-serif;
            padding: 0;
            margin: 0;
        }
        
        .container {
            width: 80%;
            margin: 0 auto;
        }
        
        .top {
            margin-top: 50px;
        }
        
        table {
            border-collapse: collapse;
            width: 50%;
        }
        
        td,
        th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }
        
        tr:nth-child(even) {
            background-color: #dddddd;
        }
    </style>
</head>

<body>
    <div class="top"></div>
    <div class="container">
        <h2>User Details</h2>

        <table>
            <tr>
                <th>User ID</th>
                <th>User Name</th>
            </tr>
            
            <% users.forEach( function(user) { %>
            <tr>
                <td> <%= user.UID %> </td>
                <td> <%= user.UNAME %> </td>
            </tr>
            <% }) %>
        </table>
    </div>


</body>

</html>